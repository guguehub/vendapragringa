{
  "name": "vendapragringa",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:guguehub/api-vendas.git",
  "author": "guzz <dekmis@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "typeorm:migration:generate": "ts-node-dev ./node_modules/typeorm/cli.js migration:generate -d src/shared/infra/typeorm/data-source.ts",
    "typeorm:migration:run": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/data-source.ts migration:run",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/data-source.ts",
    "typeorm:migration:container": "PG_HOST=db REDIS_HOST=redis APP_API_URL=localhost:3333 ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/data-source.ts",
    "dev:docker": "docker compose down && docker compose up",
    "dev:getin": "docker exec -it vendapragringa-app bash",
    "dev:container": "ts-node -r tsconfig-paths/register --transpile-only src/shared/infra/http/server.ts",
    "pgcli": "docker exec -it vendapragringa-db psql -U postgres -d apivendas",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "check:ts": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "dev:logs": "docker logs -f vendapragringa-app",
    "dev:restart": "docker-compose restart app",
    "dev:rebuild": "docker-compose up -d --build",
    "dev:check": "tsc --noEmit && yarn dev:watch",
    "dev:build": "docker compose build --no-cache",
    "dev:build:light": "docker compose build",
    "dev:build:light-reset": "rm -rf .docker/dbdata && docker compose build",
    "test:tier": "ts-node -r tsconfig-paths/register src/__tests__/subscriptions/subscription-tier-validation.test.ts",
    "dev:scrapy": "ts-node -r tsconfig-paths/register src/modules/scrap/scrapy.ts",
    "make:user_item_log": "ts-node scripts/createUserItemLogModule.ts",
  "seed:user-quotas": "ts-node -r tsconfig-paths/register src/modules/user_quota/infra/typeorm/seeds/seedUserQuotas.ts",
    "seed:missing-subscriptions": "ts-node-dev -r tsconfig-paths/register src/modules/subscriptions/infra/typeorm/seeds/seedMissingSubscriptions.ts",
    "seed:tiers": "ts-node -r tsconfig-paths/register src/modules/subscriptions/infra/typeorm/seeds/seedSubscriptions.ts",
    "seed:plans": "ts-node -r tsconfig-paths/register src/modules/products/infra/typeorm/seeds/scripts/seedPlans.ts",
    "check-schema": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:log -d src/shared/infra/typeorm/data-source.ts",
    "seed:shipping": "ts-node -r tsconfig-paths/register src/modules/shipping/infra/typeorm/seeds/runSeeds.ts",
    "seed:admin": "ts-node -r tsconfig-paths/register src/modules/users/infra/typeorm/seeds/seedAdmin.ts",
    "seed:user-items": "ts-node-dev -r tsconfig-paths/register src/modules/user_items/infra/typeorm/seeds/runSeedUserItemsAuto.ts",
    "seed:items": "ts-node-dev -r tsconfig-paths/register src/modules/item/infra/typeorm/seeds/seedItems.ts",
    "seed:suppliers": "ts-node-dev -r tsconfig-paths/register src/modules/suppliers/infra/typeorm/seeds/seedSuppliers.ts",
    "seed:users": "ts-node-dev -r tsconfig-paths/register src/modules/users/infra/typeorm/seeds/seedUsers.ts",
    "seed:core": "yarn seed:users && yarn seed:tiers && yarn seed:items && yarn seed:shipping",

      "test:quota": "ts-node src/scripts/testQuotaFlow.ts",

      "cron:test": "ts-node src/shared/infra/cron/index.ts",
      "cron:daily": "ts-node src/shared/infra/cron/dailyBonus.cron.ts",
  "cron:monthly": "ts-node src/shared/infra/cron/monthlyQuota.cron.ts",
  "typeorm:migration:subscriptions": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource ./src/shared/infra/typeorm/data-source.ts",

    "test:scrap": "bash ./scripts/test-scrap-flow.sh"



  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.18.2",
    "@types/ioredis": "4.28.10",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "1.4.5",
    "@types/node": "14.14.10",
    "@types/nodemailer": "6.4.0",
    "@types/redis": "^4.0.11",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^26.6.3",
    "prettier": "^3.0.3",
    "ts-jest": "^29",
    "ts-node": "^10.9.2",
    "ts-node-dev": "1.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.0-dev.20250218"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "aws-sdk": "^2.1621.0",
    "axios": "^1.8.2",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "chalk": "4",
    "cheerio": "1.0.0-rc.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "date-fns": "^3.3.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "express-list-endpoints": "^7.1.1",
    "express-session": "^1.18.2",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-fetch": "2",
    "nodemailer": "^6.9.8",
    "pg": "8.5.1",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^4.6.13",
    "reflect-metadata": "0.1.13",
    "tsyringe": "^4.4.0",
    "typeorm": "0.3.6",
    "uuid": "8.3.2"
  }
}
